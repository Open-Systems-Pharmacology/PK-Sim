  <Settings>
    <AllowParallelTestExecution>True</AllowParallelTestExecution>
    <SolutionConfigured>True</SolutionConfigured>
  </Settings>
  <EngineModes>
    <EngineMode>
      <Name>No Integration Tests</Name>
      <Settings>
        <TestsToExecuteAutomatically>(DoesNotHaveCategory 'ProjectConverter' AND DoesNotHaveCategory 'Reporting')</TestsToExecuteAutomatically>
      </Settings>
    </EngineMode>
  </EngineModes>
</SolutionConfiguration>